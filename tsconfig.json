{
  "compilerOptions": {
    "target": "ES6",     
    "module": "CommonJS",   
    "outDir": "./dist/",   
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/*.ts", "src/index.ts", "src/index copy.js", "src/utils/prepareData.ts"], 
  "exclude": ["node_modules"] ,
}
